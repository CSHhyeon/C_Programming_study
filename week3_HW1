#include <stdio.h>
int main(void)
{
	int N;
	scanf("%d\n", &N);
	printf("%d * 1 = %d", N, N * 1);
	printf("%d * 2 = %d", N, N * 2);
	printf("%d * 3 = %d", N, N * 3);
	printf("%d * 4 = %d", N, N * 4);
	printf("%d * 5 = %d", N, N * 5);
	printf("%d * 6 = %d", N, N * 6);
	printf("%d * 7 = %d", N, N * 7);
	printf("%d * 8 = %d", N, N * 8);
	printf("%d * 9 = %d", N, N * 9);

	return 0;
}
